<div class="jumbotron main-jumbotron">
 <a href="/"><img class="main-logo" src="/assets/logo.png"></a>
</div>

<div class="jumbotron aftermain-jumbotron">
  <% @acts.each do |act| %>
    <div class="col-md-4 col-sm-6 col-xs-12">
      <div class="act-display-name">
        <%= link_to act.name, act_path(act) %>
      </div>
      <div class="img-div">
        <%= link_to image_tag(act.image, class: "img-circle" ), act_path(act) %>
      </div>
    </div>
  <% end %>
</div>


 <div class="container">
  	<%=link_to "Add an Act", new_act_path, id: "new_act_link",  remote: true %>
  	<%=link_to "Add a Venue", new_venue_path, id: "new_venue_link", remote: true %>
 </div>
