<div class="jumbotron main-jumbotron">
 <a href="/"><img class="main-logo" src="/assets/logo.png"></a>
</div>

<div class="container">
<ul>
  <% @acts.each do |act| %>
    <li>
    <br>
      <div class="act-display">
        <%= link_to act.name, act_path(act) %>
      </div>
     </li>
  <% end %>
</ul>
</div>

Venues:

<iframe width="100%" height="400" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?visual=true&url=http%3A%2F%2Fapi.soundcloud.com%2Ftracks%2F216846955&show_artwork=true"></iframe>

<div class="container">
<ul>
  <% @venues.each do |venue| %>
    <li>
    <br>
      <div class="act-display">
        <%= link_to venue.name, venue_path(venue) %>
      </div>
     </li>
  <% end %>
</ul>
</div>


 <div class="container">
  	<%=link_to "Add an Act", new_act_path, id: "new_act_link",  remote: true %>
  	<%=link_to "Add a Venue", new_venue_path, id: "new_venue_link", remote: true %>
 </div>
