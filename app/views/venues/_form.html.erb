<%= render "layouts/errors", :object => @venue %>


<%= form_for @venue do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <br>
    <%= f.text_area :description, rows: "7", class: "form-control"%>
  </div>
  <div class="form-group">
    <%= f.label :address%>
    <%= f.text_field :address, class: "form-control"%>
  </div>
    <%= f.submit "Submit", class: "btn btn-default center-block" %>
  <% end %>